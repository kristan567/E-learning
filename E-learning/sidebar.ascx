            position: fixed;
            z-index: 1;
            top: 0;
            left: 0;
            background-color: #065036;
            overflow-x: hidden;
            padding-top: 20px;
        }

            .sidenav h1 {
                padding: 6px 8px 6px 16px;
                text-decoration: none;
                color: white;
                display: block;
            }

            .sidenav a {
                padding: 6px 8px 6px 16px;
                text-decoration: none;
                font-size: 25px;
                color: white;
                display: block;
            }

                .sidenav a:hover {
                    color: #f1f1f1;
                }

        BoundField {
            background-color: #065036;
        }

        .main {
            margin-left: 250px; /* Same as the width of the sidenav */

            padding: 0px 10px;
        }

        .custom-color {
            background-color: #065036; /* Change to the desired color */
            color: white;
        }

        @media screen and (max-height: 450px) {
            .sidenav {
                padding-top: 15px;
            }

                .sidenav a {
                    font-size: 18px;
                }
        }
    </style>


